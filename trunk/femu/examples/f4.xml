<?xml version="1.0"?>
<feynman-rules name="F4 self-interacting theory">
    <particles>
        <particle name="scalar boson" line-style="normal"/>
    </particles>
    <interactions>
        <interaction name="f4 interaction vertex" allow-simple-loops="yes">
            <participant particle="scalar boson"/>
            <participant particle="scalar boson"/>
            <participant particle="scalar boson"/>
            <participant particle="scalar boson"/>
        </interaction>
    </interactions>
</feynman-rules>
